<?php
// ループ中に、もしdogだったらワンと出力してください
$animals = array("dog", "cat", "panda");
foreach ($animals as $animal) {
  // code...
  if ($animal == "dog") {
    // code...
    echo "ワン".PHP_EOL;
  }
}

$start = 1;
$end = 100;
for ($i= $start; $i <= $end ; $i++) {
  // code...
  if ($i %5 == 0) {
    // code...
    echo $i;
  }
}
